<?xml version="1.0"?>
<launch>
  
  <arg name="imav_indoor_xacro" default="$(find imav_2017)/worlds/xacro/imav_indoor.world.xacro"/>
  <param name="imav_indoor_world" command="$(find xacro)/xacro --inorder '$(arg imav_indoor_xacro)'" />
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    
    
    <arg name="world_name" value="$(param imav_indoor_world)"/>
  </include>


  <!-- Spawn simulated quadrotor uav -->
  <include file="$(find imav_2017)/launch/spawn_quadrotor.launch" >
    <!--
    <arg name="model" value="$(find imav_2017)/urdf/quadrotor_sensors.urdf.xacro"/> 
    -->
  </include>
</launch>
